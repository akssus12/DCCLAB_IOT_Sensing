<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="dcclab">
	<select id="insertCpuInfo">
		<![CDATA[
			INSERT INTO tb_cpuinfo(cpuNm,cpuTemp,cpuLoad,crtDttm)
			VALUES(#{cpuNm},#{cpuTemp},#{cpuLoad},#{crtDttm})
		]]>
	</select>

</mapper>
